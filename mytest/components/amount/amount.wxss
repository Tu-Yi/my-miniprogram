.num_wrap {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;

}
.num_wrap .btn{
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.num_wrap .text {
  box-sizing: border-box;
  width: 60rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}
.btn::after{
  border: none;
}